						--]
						End()
						
						-- 미세스본 방향에 본장군의 방향 복사
						Action( "calculate" )
						--[
							Param( "express", "b3MrsDir = b3BonDir" )
						--]
						End()
						
						-- 본장군 기준 10미터 거리에 
						Action( "calculate" )
						--[
							Param( "express", "b3MrsLoc = RelativeCoord( b3BonLoc, b3BonDir, 120, 10 )" )
