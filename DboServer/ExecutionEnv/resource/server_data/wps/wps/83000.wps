	Action( "CCBD stage" )
	--[
		Param( "stage", 5 )
		Param( "direct play", "false" )


		-----------------------------------
		-- 보스 1

		-- Engineer spawn
		Action( "add mobgroup")
		--[
			Param("group", 1104 )
			Param("no spawn wait", "true" )
			Param("respawn", "true")
		--]
		End()
		
		-- The descendants of Jiran spawn (boss)
		Action( "add mobgroup")
		--[
			Param("group", 4500 )
			Param("no spawn wait", "true" )
		--]
		End()

		Action("function")
		--[
			Condition("child")
			--[
				-- 기란의 후예 battle 들어갔는지 확인
				Action("wait")
				--[
					Condition("check battle")
					--[
						Param("type", "mob")
						Param("group", 4500 )
						Param("is battle", "true")
					--]
					End()
				--]
				End()
				
				-- 기란의 후예 LP 70%이면
				Action( "wait" )
				--[
					Condition( "check lp" )
					--[
						Param( "type", "mob" )
						Param( "group", 4500 )
						Param( "index", 3411215 )
						Param( "lp", 70 )
					--]
					End()
				--]
				End()
				
				-- 임시
				-- system message '기란의 후예 LP 70% 순찰 다니기 시작'
				Action( "system message" )
				--[
					Param( "notify", "notice" )
					Param( "text index", 47 )
				--]
				End()
													
				Action( "system message" )
				--[
					Param( "notify", "system" )
					Param( "text index", 47 )
				--]
				End()
				
				
				
				-- 지하기지 호위무사 순찰 spawn
				Action("add mobgroup")
				--[
					Param("group", 1101 )
					Param("no spawn wait", "true")
					Param("respawn", "true")
				--]
				End()

				-- 지하기지 호위무사 순찰 spawn
				Action("add mobgroup")
				--[
					Param("group", 1102 )
					Param("no spawn wait", "true")
					Param("respawn", "true")
				--]
				End()
				
			--]
			End()
			
			-- 기란의 후예 사망하면
			Action("wait")
			--[
				Condition("check mobgroup")
				--[
					Param("group", 4500 )
					Param("count", 0 )
				--]
