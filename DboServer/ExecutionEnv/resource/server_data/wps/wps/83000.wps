		--]
		End()


		Action( "function" )
		--[
			Condition( "child" )
			--[
				Action( "while" )
				--[
					Action( "loop" )
					--[
						-- 전투상태가 풀리면 안쪽 loop에서 탈출하고, 스폰한 mob 제거함
						Action( "wait" )
						--[
							Condition( "check battle" )
							--[
								Param( "type", "mob" )
								Param( "group", 1301 )
								Param( "is battle", "true" )
							--]
							End()
						--]
						End()
						
						-- 본장군의 LP가 70%가 되면 
						Action( "wait" )
						--[
							Condition( "check lp" )
							--[
								Param( "type", "mob" )
								Param( "group", 1301 )
								Param( "lp", 70 )
							--]
